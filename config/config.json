{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "gurukul-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/gurukul/GurukulWebPart.js",
          "manifest": "./src/webparts/gurukul/GurukulWebPart.manifest.json"
        }
      ]
    },
    "class-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/class/ClassWebPart.js",
          "manifest": "./src/webparts/class/ClassWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "GurukulWebPartStrings": "lib/webparts/gurukul/loc/{locale}.js",
    "PersonWebPartStrings": "lib/webparts/person/loc/{locale}.js",
    "ClassWebPartStrings": "lib/webparts/class/loc/{locale}.js"
  }
}
